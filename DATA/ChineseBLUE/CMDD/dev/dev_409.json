        },
        {
            "speaker": "医生",
            "sentence": "嗯，的确是不好",
            "label": "OOOOOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "医生",
            "sentence": "益生菌需要吃一段时间",
            "label": "OOOOOOOOOO",
