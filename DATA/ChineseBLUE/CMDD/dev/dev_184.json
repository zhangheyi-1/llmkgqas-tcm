        },
        {
            "speaker": "医生",
            "sentence": "量大不大？",
            "label": "OOOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "医生",
            "sentence": "有没有比平时稀一点？",
            "label": "OOOOOOOOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "患者",
            "sentence": "还行，都是稀的",
            "label": "OOOOOBO",
