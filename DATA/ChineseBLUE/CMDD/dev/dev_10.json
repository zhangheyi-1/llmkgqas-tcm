            "normalized": [
                "支气管炎"
            ],
            "type": [
                "1"
            ]
        },
        {
            "speaker": "医生",
            "sentence": "没脾脏吗",
            "label": "OOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "患者",
