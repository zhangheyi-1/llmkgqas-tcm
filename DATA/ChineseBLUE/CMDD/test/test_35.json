    "dialogue-content": [
        {
            "speaker": "医生",
            "sentence": "您好，我是您的接诊医生，需要询问几个问题，才能更好的评估孩子情况，您还在吗？",
            "label": "OOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "医生",
            "sentence": "宝宝这样多久了",
