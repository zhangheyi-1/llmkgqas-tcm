            "type": []
        },
        {
            "speaker": "患者",
            "sentence": "上星期六上医院检查血",
            "label": "OOOOOOOOOO",
            "normalized": [],
            "type": []
        },
        {
            "speaker": "医生",
            "sentence": "宝宝气喘吗？",
